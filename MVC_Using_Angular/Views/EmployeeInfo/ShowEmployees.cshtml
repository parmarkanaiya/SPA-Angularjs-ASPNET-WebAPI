<h2>List of Employees</h2>
<hr />
<a ng-click="addEmployee()" class="btn-link" > Add Employee </a>
<br />
<table class="table table-responsive table-striped" >
        <tr >
            <th>EmpNo</th><th>EmpName</th><th>Salary</th><th>DeptName</th><th>Designation</th><th>Actions</th>
        </tr>
        <tr  ng-repeat="Emp in Employees">
            <td>{{Emp.EmpNo}}</td>
            <td>{{Emp.EmpName}}</td>
            <td>{{Emp.Salary}}</td>
            <td>{{Emp.DeptName}}</td>
            <td>{{Emp.Designation}}</td>
            <td>
                <button ng-click="editEmployee(Emp.EmpNo)" class="btn btn-link" value="Edit"  >Edit</button>
                <button ng-click="deleteEmployee(Emp.EmpNo)" class="btn btn-link" value="Delete">Delete</button>
            </td>
        </tr>
</table>


 
<div>{{error}}</div>

 

