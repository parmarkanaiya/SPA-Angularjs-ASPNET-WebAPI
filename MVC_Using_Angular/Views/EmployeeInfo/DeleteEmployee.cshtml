
@{
    ViewBag.Title = "DeleteEmployee";
}

<h2>@ViewBag.Title</h2>
<hr />

<table class="table">
    <tr>
        <td>EmpNo</td>
        <td>{{Employee.EmpNo}}</td>
    </tr>
    <tr>
        <td>EmpName</td>
        <td>{{Employee.EmpName}}</td>
    </tr>
    <tr>
        <td>Salary</td>
        <td>{{Employee.Salary}}</td>
    </tr>
    <tr>
        <td>DeptName</td>
        <td>{{Employee.DeptName}}</td>
    </tr>
    <tr>
        <td>Designation</td>
        <td>{{Employee.Designation}}</td>
    </tr>
</table>
<input type="button" class="btn btn-primary" value="Delete" ng-click="delete()" />
<input type="button" value="Cancel" ng-click="cancel()" class="btn btn-link" />

